       );","    }","}"],"fileName":"ProfileComponent.js","type":"file"},"react.user_manager_02.js.sample":{"fileType":"js","content":["import Jivo from 'rtn-jivo-sdk';","","class UserManager {","    constructor() {","        this.user = null","    }","","    updateUser(user) {","        this.user = user","        this._configureJivoSession()","    }","","    _configureJivoSession() {","        if (this.user) {","            const info = {","                'name': this.user.full_name,","                'email': this.user.email,","                'phone': this.user.phone_number","            }","            ","            Jivo.session.setup(\"YOUR_CHANNEL_ID\", this.user.jwt)","            Jivo.session.setContactInfo(info)","        }","        else {","            Jivo.session.shutDown()","        }","    }","}"],"type":"file","identifier":"react.user_manager_02.js.sample","fileName":"UserManager.js","highlights":[{"line":15},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":22}],"syntax":"js"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/flutter_usage_basics#How-to-display-SDK":{"title":"How to display SDK","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/flutter_usage_basics#How-to-display-SDK","type":"section","kind":"section","abstract":[{"text":"Some scenarios of how to use SDK","type":"text"}],"url":"\/tutorials\/jivosdk\/flutter_usage_basics#How-to-display-SDK","role":"pseudoSymbol"},"doc://org.cocoapods.JivoSDK/tutorials/table":{"title":"JivoSDK Samples","type":"topic","url":"\/tutorials\/table","role":"overview","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table","abstract":[],"kind":"overview"},"react.app_delegate_01.swift.sample":{"fileName":"AppDelegate.swift","highlights":[{"line":3},{"line":19},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30},{"line":31},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39}],"identifie