cation: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    override func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","}"],"syntax":"swift","fileType":"swift"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/react_usage_basics":{"title":"Basic usage","kind":"project","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"url":"\/tutorials\/jivosdk\/react_usage_basics","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/react_usage_basics","type":"topic","role":"project"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics#How-to-handle-Push-Notifications":{"role":"pseudoSymbol","url":"\/tutorials\/jivosdk\/native_usage_basics#How-to-handle-Push-Notifications","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"type":"section","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics#How-to-handle-Push-Notifications","kind":"section","title":"How to handle Push Notifications"},"doc://org.cocoapods.JivoSDK/tutorials/table/Flutter-app":{"title":"Flutter app","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table\/Flutter-app","type":"topic","kind":"article","abstract":