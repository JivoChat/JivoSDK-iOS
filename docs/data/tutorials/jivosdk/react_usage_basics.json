n()","    }","","    _configureJivoSession() {","        if (this.user) {","            const info = {","                'name': this.user.full_name,","                'email': this.user.email,","                'phone': this.user.phone_number","            }","            ","            Jivo.startUp(\"YOUR_CHANNEL_ID\", this.user.supportJwt)","            Jivo.setContactInfo(info)","        }","        else {","            Jivo.shutDown()","        }","    }","}"],"highlights":[{"line":15},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":22}],"type":"file","syntax":"js","fileType":"js","identifier":"react.user_manager_02.js.sample","fileName":"UserManager.js"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/flutter_usage_basics#How-to-display-SDK":{"url":"\/tutorials\/jivosdk\/flutter_usage_basics#How-to-display-SDK","title":"How to display SDK","type":"section","kind":"section","role":"pseudoSymbol","abstract":[{"text":"Some scenarios of how to use SDK","type":"text"}],"identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/flutter_usage_basics#How-to-display-SDK"},"doc://org.cocoapods.JivoSDK/tutorials/table/Native-app":{"kind":"article","type":"topic","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table\/Native-app","abstract":[],"role":"article","title":"Native app","url":"\/tutorials\/table\/native-app"},"react.user_manager_00.js.sample":{"fileType":"js","type":"file","highlights":[],"content":["class UserManager {","    constructor() {","        this.user = null","    }","","    updateUser(user) {","        this.user = user","    }","}"],"fileName":"UserManager.js","syntax":"js","identifier":"react.user_manager_00.js.sample"},"react.app_delegate_02.swift.sample":{"content":["import Foundation","import UIKit","import UserNotifications","import JivoSDK","","#if DEBUG","#if FB_SONARKIT_ENABLED","import FlipperKit","#endif","#endif","","@UIApplicationMain","final class AppDelegate: UIResponder",", UIApplicationDelegate",", UNUs