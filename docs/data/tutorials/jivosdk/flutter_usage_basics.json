lay-SDK","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}]},"flutter.app_delegate_02.swift.sample":{"type":"file","fileName":"AppDelegate.swift","syntax":"swift","content":["import UIKit","import UserNotifications","import Flutter","import JivoSDK","","@UIApplicationMain","@objc class AppDelegate: FlutterAppDelegate {","    override func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","        UNUserNotificationCenter.current().delegate = self","        GeneratedPluginRegistrant.register(with: self)","        ","        if Jivo.notifications.handleLaunch(options: launchOptions) {","            \/\/ Launched by JivoSDK notification","        }","        ","        return super.application(application, didFinishLaunchingWithOptions: launchOptions)","    }","    ","    override func application(_ application: UIApplication, didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Jivo.notifications.setPushToken(data: deviceToken)","    }","    ","    override func application(_ application: UIApplication, didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(data: nil)","    }","    ","    override func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            \/\/ Notification for JivoSDK, just pass this result into completionHandler","            completionHandler(result)","        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    override func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","}"],"identifier":"flutter.app_delegate_02.swift.sample","fileType":"swift","highlights":[{"line":2},{"line":9},{"line":24},{"line":28},{"line":29},{"line":30},{"line":31},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40},{"line":41},{"line":42},{"line":43},{"line":44},{"line":45},{"line":46},{"line":47},{"line":48},{"line":49}]},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/react_usage_basics":{"abstract":[{"text":"Some scenarios of how to use SDK","type":"text"}],"title":"Basic usage","url":"\/tutorials\/jivosdk\/react_usage_basics","role":"project","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/react_usage_basics","type":"topic","kind":"project"},"flutter.user_manager_02.dart.sample":{"content":["import 'package:jivosdk_plugin\/bridge.dart';","","class User {","    String email;","    String jwt;","    String? fullName;","    String? phoneNumber;","    User(this.email, this.jwt, this.fullName, this.phoneNumber);","}","","class UserManager {","    User? user;","    ","    void updateUser(User? user) {","        this.user = user;","        _configureJivoSession();","    }","  ","    void _configureJivoSession() {","        if (user != null) {","            Jivo.session.startUp(","                channelID: 'YOUR_CHANNEL_ID',","                userToken: user?.jwt ?? '');","          ","            Jivo.session.setContactInfo(","                name: user?.fullName,","                email: user?.email,","                phone: user?.phoneNumber,","                brief: \"Awesome person\");","        }","        else {","            Jivo.session.shutDown();","        }","    }","}"],"type":"file","identifier":"flutter.user_manager_02.dart.sample","fileName":"UserManager.dart","fileType":"dart","highlights":[{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29}],"syntax":"dart"},"flutter.app_delegate_00.swift.sample":{"content":["import UIKit","import Flutter","","@UIApplicationMain","@objc class AppDelegate: FlutterAppDelegate {","    override func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","        GeneratedPluginRegistrant.register(with: self)","        return super.application(application, didFinishLaunchingWithOptions: launchOptions)","    }","}"],"fileType":"swift","fileName":"AppDelegate.swift","type":"file","highlights":[],"identifier":"flutter.app_delegate_00.swift.sample","syntax":"swift"},"doc://org.cocoapods.JivoSDK/tutorials/table/React-Native-app":{"type":"topic","kind":"article","role":"article","url":"\/tutorials\/table\/react-native-app","title":"React Native app","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table\/React-Native-app","abstract":[]},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/flutter_usage_basics#How-to-display-SDK":{"abstract":[{"text":"Some scenarios of how to use SDK","type":"text"}],"url":"\/tutorials\/jivosdk\/flutter_usage_basics#How-to-display-SDK","role":"pseudoSym