"        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        Jivo.notifications.didReceive(response: response)","        completionHandler()","    }","}"],"identifier":"native.app_delegate_02.swift.sample","type":"file"},"native.user_manager_00.swift.sample":{"fileName":"UserManager.swift","type":"file","identifier":"native.user_manager_00.swift.sample","fileType":"swift","highlights":[],"syntax":"swift","content":["import Foundation","","struct User {","    let email: String","    let supportJwt: String","    let fullName: String?","    let phoneNumber: String?","}","","final class UserManager {","    private(set) var user: User?","    ","    func update(user: User) {","        self.user = user","    }","}"]},"doc://org.cocoapods.JivoSDK/tutorials/table/Native-app":{"url":"\/tutorials\/table\/native-app","role":"article","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table\/Native-app","abstract":[],"kind":"article","title":"Native app","type":"topic"},"native.profile_view_controller_00.swift.sample":{"hig