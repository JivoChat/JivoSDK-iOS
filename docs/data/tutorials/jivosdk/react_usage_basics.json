ed by JivoSDK notification","        }","        ","        UNUserNotificationCenter.current().delegate = self","        return true","    }","    ","    func application(_ application: UIApplication, didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Jivo.notifications.setPushToken(data: deviceToken)","    }","    ","    func application(_ application: UIApplication, didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(data: nil)","    }","    ","    func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            \/\/ Notification for JivoSDK, just pass this result into completionHandler","            completionHandler(result)","        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","    ","    private func initializeFlipper(with application: UIApplication) {","        \/\/ ...","    }","}"],"identifier":"react.app_delegate_02.swift.sample","syntax":"swift","fileType":"swift","type":"file"},"react.profile_component_00.js.sample":{"identifier":"react.profile_component_00.js.sample","syntax":"js","content":["import React from 'react';","import { SafeAreaView } from 'react-native';","","export default class ProfileComponent extends React.Component {","    render() {","        return (","            <SafeAreaView>","            <\/SafeAreaView>","        );","    }","}"],"type":"file","fileType":"js","fileName":"ProfileComponent.js","highlights":[]},"doc://org.cocoapods.JivoSDK/tutorials/table/Flutter-app":{"title":"Flutter app","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table\/Flutter-app","kind":"article","url":"\/tutorials\/table\/flutter-app","type":"topic","role":"article","abstract":[]},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/flutter_usage_basics#How-to-display-SDK":{"abstract":[{"text":"Some scenarios of how to use SDK","type":"text"}],"url":"\/tutorials\/jivosdk\/flutter_usage_basics#How-to-display-SDK","role":"pseudoSymbol","title":"How to display SDK","kind":"section","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/flutter_usage_basics#How-to-display-SDK","type":"section"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/react_usage_basics":{"url":"\/tutorials\/jivosdk\/react_usage_basics","type":"topic","kind":"project","role":"project","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/react_usage_basics","title":"Basic usage","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}]},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics":{"url":"\/tutorials\/jivosdk\/native_usage_basics","ident