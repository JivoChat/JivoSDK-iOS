ilToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(data: nil)","    }","    ","    override func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            completionHandler(result)","        }","        else {","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            completionHandler(options)","        }","        else {","            completionHandler([])","        }","    }","    ","    override func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        Jivo.notifications.didReceive(response: response)","        completionHandler()","    }","}"],"identifier":"flutter.app_delegate_02.swift.sample","type":"file","fileType":"swift"},"flutter.user_manager_02.dart.sample":{"content":["import 'package:jivosdk_plugin\/bridge.dart';","","class User {","    String email;","    String supportJwt;","    String? fullName;","    String? phoneNumber;","    User(this.email, this.supportJwt, this.fullName, this.phoneNumber);","}","","class UserManager {","    User? user;","    ","    void updateUser(User? user) {","        this.user = user;","        _configureJivoSession();","    }","  ","    void _configureJivoSession() {","        if (user != null) {","            Jivo.session.startUp(","                channelID: 'YOUR_CHANNEL_ID',","                userToken: user?.supportJwt ?? '');","          ","            Jivo.session.setContactInfo(","                name: user?.fullName,","                email: user?.email,","                phone: user?.phoneNumber,","                brief: \"Awesome person\");","        }","        else {","            Jivo.session.shutDown();","        }","    }","}"],"fileName":"UserManager.dart","fileType":"dart","identifier":"flutter.user_manager_02.dart.sample","highlights":[{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29}],"syntax":"dart","type":"file"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/react_usage_basics#How-to-handle-user-session":{"type":"section","role":"pseudoSymbol","url":"\/tutorials\/jivosdk\/react_usage_basics#How-to-handle-user-session","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/react_usage_basics#How-to-handle-user-session","title":"How to handle user session","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"kind":"section"},"flutter.profile_widget_02.dart.sample":{"fileName":"ProfileWidget.dart","highlights":[{"line":2},{"line":16}],"syntax":"dart","content":["import 'package:flutter\/material.dart';","import 'package:jivosdk_plugin\/bridge.dart';","","class ProfileWidget extends StatelessWidget {","    const ProfileWidget({super.key});","    ","    @override","    Widget build(BuildContext context) {","        return Scaffold(","            appBar: AppBar(","                title: const Text('Profile')","            ),","            floatingActionButton: FloatingActionButton(","                child: const Icon(Icons.support),","                onPressed: () {","                    Jivo.display.present();","                }","            ),","        );","    }","}"],"identifier":"flutter.profile_widget_02.dart.sample","type":"file","fileType":"dart"},"flutter.profile_widget_00.dart.sample":{"fileName":"ProfileWidget.dart","fileType":"dart","content":["im