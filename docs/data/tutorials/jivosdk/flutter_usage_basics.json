r: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    override func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","}"],"fileName":"AppDelegate.swift","fileType":"swift","syntax":"swift","highlights":[{"line":2},{"line":9},{"line":24},{"line":28},{"line":29},{"line":30},{"line":31},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40},{"line":41},{"line":42},{"line":43},{"line":44},{"line":45},{"line":46},{"line":47},{"line":48},{"line":49}]},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics#How-to-display-SDK":{"type":"section","kind":"section","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"role":"pseudoSymbol","title":"How to display SDK","url":"\/tutorials\/jivosdk\/native_usage_basics#How-to-display-SDK","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics#How-to-display-SDK"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics#How-to-customize-UI-elements":{"kind":"section","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics#How-to-customize-UI-elements","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"role":"pseudoSymb