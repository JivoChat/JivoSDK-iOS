:16},{"line":17},{"line":18},{"line":19},{"line":20}]},"react.profile_component_00.js.sample":{"content":["import React from 'react';","import { SafeAreaView } from 'react-native';","","export default class ProfileComponent extends React.Component {","    render() {","        return (","            <SafeAreaView>","            <\/SafeAreaView>","        );","    }","}"],"highlights":[],"fileName":"ProfileComponent.js","type":"file","fileType":"js","identifier":"react.profile_component_00.js.sample","syntax":"js"},"react.user_manager_02.js.sample":{"identifier":"react.user_manager_02.js.sample","type":"file","fileType":"js","content":["import Jivo from 'rtn-jivo-sdk';","","class UserManager {","    constructor() {","        this.user = null","    }","","    updateUser(user) {","        this.user = user","        this._configureJivoSession()","    }","","    _configureJivoSession() {","        if (this.user) {","            const info = {","                'name': this.user.full_name,","                'email': this.user.email,","                'phone': this.user.phone_number","            }","            ","            Jivo.session.setup(\"YOUR_CHANNEL_ID\", this.user.jwt)","            Jivo.session.setContactInfo(info)","        }","        else {","            Jivo.session.shutDown()","        }","    }","}"],"fileName":"UserManager.js","syntax":"js","highlights":[{"line":15},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":22}]},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/flutter_usage_basics#How-to-handle-Push-Notifications":{"kind":"section","url":"\/tutorials\/jivosdk\/flutter_usage_basics#How-to-handle-Push-Notifications","role":"pseudoSymbol","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/flutter_usage_basics#How-to-handle-Push-Notifications","type":"section","title":"How to handle Push Notifications","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}]},"react.user_manager_00.js.sample":{"type":"file","highlights":[],"fileName":"UserManager.js","fileType":"js","identifier":"react.user_manager_00.js.sample","syntax":"js","content":["class UserManager {","    constructor() {","        this.user = null","    }","","    updateUser(user) {","        this.user = user","    }","}"]},"react.app_delegate_02.swift.sample":{"fileName":"AppDelegate.swift","highlights":[{"line":3},{"line":14},{"line":15},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":34},{"line":39},{"line":43},{"line":47},{"line":48},{"line":49},{"line":50},{"line":51},{"line":52},{"line":53},{"line":54},{"line":55},{"line":56},{"line":57},{"line":58},{"line":59},{"line":60},{"line":61},{"line":62},{"line":63},{"line":64},{"line":65},{"line":66}],"type":"file","identifier":"react.app_delegate_02.swift.sample","syntax":"swift","fileType":"swift","content":["import Foundation","import UIKit","import UserNotifications","import JivoSDK","","#if DEBUG","#if FB_SONARKIT_ENABLED","import FlipperKit","#endif","#endif","","@UIApplicationMain","final class AppDelegate: UIResponder",", UIApplicationDelegate",", UNUserNotificationCenterDelegate {","    func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil) -> Bool {","        initializeFlipper(with: application)","        ","        \/\/ ...","        ","        if Jivo.notifications.handleLaunch(options: launchOptions) {","            \/\/ Launched by JivoSDK notification","        }","        ","        UNUserNotificationCenter.current().delegate = self","        return true","    }","    ","    func application(_ application: UIApplication, didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Jivo.notifications.setPushToken(data: deviceToken)","    }","    ","    func application(_ application: UIApplication, didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(data: nil)","    }","    ","    func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            \/\/ Notification for JivoSDK, just pass this result into completionHandler","            completionHandler(result)","        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","    ","    private func initializeFlipper(with application: UIApplication) {","        \/\/ ...","    }","}"]},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/react_usage_basics#How-to-display-SDK":{"kind":"section","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"url":"\/tutorials\/jivosdk\/react_usage_basics#How-to-display-SDK","r