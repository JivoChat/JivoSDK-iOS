    }","}"],"fileName":"ProfileViewController.swift","fileType":"swift","type":"file"},"native.app_delegate_00.swift.sample":{"content":["import Foundation","import UIKit","","final class AppDelegate: UIResponder",", UIApplicationDelegate {","    func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil) -> Bool {","        return true","    }","}"],"fileName":"AppDelegate.swift","fileType":"swift","identifier":"native.app_delegate_00.swift.sample","highlights":[],"syntax":"swift","type":"file"},"doc://org.cocoapods.JivoSDK/tutorials/table/Flutter-app":{"title":"Flutter app","type":"topic","role":"article","kind":"article","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table\/Flutter-app","abstract":[],"url":"\/tutorials\/table\/flutter-app"},"native.profile_view_01.swift.sample":{"content":["import Foundation","import SwiftUI","import JivoSDK","","struct ProfileView: some View {","    @State private var shouldPresentChat = false","    ","    var body: some View {","        Button(action: {shouldPresentChat.toggle()}) {","            Text(\"Tech Support\")","        }","        .fullScreenCover(isPresented: $shouldPresentChat) {","            Jivo.display.makeScreen(.modal)","        }","    }","}"],"fileType":"swift","syntax":"swift","highlights":[],"type":"file","identifier":"native.profile_view_01.swift.sample","fileName":"ProfileView.swift"},"native.app_delegate_02.swift.sample":{"fileType":"swift","syntax":"swift","highlights":[{"line":4},{"line":11},{"line":12},{"line":13},{"line":14},{"line":15},{"line":20},{"line":24},{"line":28},{"line":29},{"line":30},{"line":31},{"line":32},{"line":33},{"line":34},{"line":35},{"line":39},{"line":40},{"line":41},{"line":42},{"line":43},{"line":44},{"line":45},{"line":46},{"line":50},{"line":51},{"line":52},{"line":53}],"content":["import Foundation","import UIKit","import UserNotifications","import JivoSDK","","final class AppDelegate: UIResponder",", UIApplicationDelegate",", UNUserNotificationCenterDelegate {","    func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil) -> Bool {","        UNUserNotificationCenter.current().delegate = self","        ","        if Jivo.notifications.handleLaunch(options: launchOptions) {","            \/\/ Launched by JivoSDK notification","        }","        ","        return true","    }","    ","    func application(_ application: UIApplication, didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Jivo.notifications.setPushToken(data: deviceToken)","    }","    ","    func application(_ application: UIApplication, didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(error: error)","    }","    ","    func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            \/\/ Notification for JivoSDK, just pass this result into completionHandler","            completionHandler(result)","        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","}"],"fileName":"AppDelegate.swift","type":"file","identifier":"native.app_delegate_02.swift.sample"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/flutter_usage_basics":{"url":"\/tutorials\/jivosdk\/flutter_usage_basics","kind":"project","abstract":[{"text":"Some scenarios of how to use SDK","type":"text"}],"title":"Basic usage","role":"project","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/flutter_usage_basics","type":"topic"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics#How-to-handle-Push-Notifications":{"role":"pseudoSymbol","url":"\/tutorials\/jivosdk\/native_usage_basics#How-to-handle-Push-Notifications","type":"section","title":"How to handle Push Notifications","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"kind":"section","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics#How-to-handle-Push-Notifications"},"doc://org.cocoapods.JivoSDK/documentation/JivoSDK/JVDisplayElement":{"abstract":[{"type":"text","text":"UI elements available for customization"}],"type":"topic","title":"JVDisplayElement","identifier":"doc:\/\/org.cocoapods.JivoSDK\/documentation\/JivoSDK\/JVDisplayElement","kind":"symbol","url":"\/documentation\/jivosdk\/jvdisplayelement","navigatorTitle":[{"text":"JVDisplayElement","kind":"identifier"}],"fragments":[{"text":"enum","kind":"keyword"},{"text":" ","kind":"text"},{"text":"JVDisplayElement","kind":"identifier"}],"role":"symbol"},"doc://o