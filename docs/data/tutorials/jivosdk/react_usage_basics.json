ine":38},{"line":39}],"fileName":"AppDelegate.swift"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics#How-to-handle-user-session":{"identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics#How-to-handle-user-session","type":"section","title":"How to handle user session","role":"pseudoSymbol","url":"\/tutorials\/jivosdk\/native_usage_basics#How-to-handle-user-session","kind":"section","abstract":[{"text":"Some scenarios of how to use SDK","type":"text"}]},"react.app_delegate_00.swift.sample":{"content":["import Foundation","import UIKit","","#if DEBUG","#if FB_SONARKIT_ENABLED","import FlipperKit","#endif","#endif","","@UIApplicationMain","final class AppDelegate: UIResponder",", UIApplicationDelegate {","    func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil) -> Bool {","        initializeFlipper(with: application)","        ","        \/\/ ...","        ","        return true","    }","    ","    private func initializeFlipper(with application: UIApplication) {","        \/\/ ...","    }","}"],"highlights":[],"type":"file","syntax":"swift","fileType":"swift","identifier":"react.app_delegate_00.swift.sample","fileName":"AppDelegate.swift"},"doc://org.cocoapods.JivoSDK/tutorials/table/Flutter-app":{"type":"topic","abstract":[],"title":"Flutter app","url":"\/tutorials\/table\/flutter-app","role":"article","kind":"article","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table\/Flutter-app"},"react.user_manager_00.js.sample":{"fileName":"UserManager.js","content":["class UserManager {","    constructor() {","        this.user = null","    }","","    updateUser(user) {","        this.user = user","    }","}"],"identifier":"react.user_manager_00.js.sample","highlights":[],"type":"file","fileType":"js","syntax":"js"},"react.user_manager_02.js.sample":{"fileType":"js","content":["import Jivo from 'rtn-jivo-sdk';","","class UserManager {","    constructor() {","        this.user = null","    }","","    updateUser(user) {","        this.user = user","        this._configureJivoSession()","    }","","    _configureJivoSession() {","        if (this.user) {","            const info = {","                'name': this.user.full_name,","                'email': this.user.email,","                'phone': this.user.phone_number","            }","            ","            Jivo.session.setup(\"YOUR_CHANNEL_ID\", this.user.jwt)","            Jivo.session.setContactInfo(info)","        }","        else {","            Jivo.session.shutDown()","        }","    }","}"],"syntax":"js","fileName":"UserManager.js","type":"file","identifier":"react.user_manager_02.js.sample","highlights":[{"line":15},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":22}]},"react.app_delegate_02.swift.sample":{"fileType":"swift","fileName":"AppDelegate.swift","type":"file","syntax":"swift","content":["import Foundation","import UIKit","import UserNotifications","import JivoSDK","","#if DEBUG","#if FB_SONARKIT_ENABLED","import FlipperKit","#endif","#endif","","@UIApplicationMain","final class AppDelegate: UIResponder",", UIApplicationDelegate",", UNUserNotificationCenterDelegate {","    func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil) -> Bool {","        initializeFlipper(with: application)","        ","        \/\/ ...","        ","        if Jivo.notifications.handleLaunch(options: launchOptions) {","            \/\/ Launched by JivoSDK notification","        }","        ","        UNUserNotificationCenter.current().delegate = self","        return true","    }","    ","    func application(_ application: UIApplication, didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Jivo.notifications.setPushToken(data: deviceToken)","    }","    ","    func application(_ application: UIApplication, didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(data: nil)","    }","    ","    func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            \/\/ Notification for JivoSDK, just pass this result into completionHandler","            completionHandler(result)","        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","    ","    private func initializeFlipper(with application: UIApplication) {","        \/\/ ...","    }","}"],"identifier":"react.app_delegate_02.swift.sample","highlights":[{"line":3},{"line":14},{"line":15},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":34},{"line":39},{"line":43},{"line":47},{"line":48},{"line":49},{"line":50},{"line":51},{"line":52},{"line":53},{"line":54},{"line":55},{"line":56},{"line":57},{"line":58},{"line":59},{"line":60},{"line":61},{"line":62},{"line":63},{"line":64},{"line":65},{"line":66}]},"react.user_manager_01.js.sample":{"fileName":"UserManager.js","type":"file","content":["import Jivo from 'rtn-jivo-sdk';","","class UserManager {","    constructor() {","        this.user = null","    }","","    updateUser(user) {","        this.user = user","        this._configureJivoSession()","    }","","    _configureJivoSession() {","        if (this.user) {","            Jivo.session.setup(\"YOUR_CHANNEL_ID\", this.user.jwt)","        }","        else {","            Jivo.session.shutDown()","        }","    }","}"],"syntax":"js","identifier":"react.user_manager_01.js.sample","highlights":[{"line":1},{"line":2},{"line":10},{"line":12},{"line":13},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20}],"fileType":"js"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/react_usage_basics#How-to-handle-Push-Notifications":{"url":"\/tutorials\/jivosdk\/react_usage_basics#How-to-handle-Push-Notifications","kind":"section","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"title":"How to handle Push Notifications","role":"pseudoSymbol","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/react_usage_basics#How-to-handle-Push-Notifications","type":"section"},"react.profile_component_00.js.sample":{"identifier":"react.profile_component_00.js.sample","content":["import React from 'react';","import { SafeAreaView } from 'react-native';","","export default class ProfileComponent extends React.Component {","    render() {","        return (","            <SafeAreaView>","            <\/SafeAreaView>","        );","    }","}"],"type":"file","fileName":"ProfileComponent.js","highlights":[],"fileType":"js","syn