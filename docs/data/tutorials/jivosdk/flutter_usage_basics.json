ion, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","        UNUserNotificationCenter.current().delegate = self","        GeneratedPluginRegistrant.register(with: self)","        ","        if Jivo.notifications.handleLaunch(options: launchOptions) {","            \/\/ Launched by JivoSDK notification","        }","        ","        return super.application(application, didFinishLaunchingWithOptions: launchOptions)","    }","    ","    override func application(_ application: UIApplication, didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Jivo.notifications.setPushToken(data: deviceToken)","    }","    ","    override func application(_ application: UIApplication, didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(data: nil)","    }","    ","    override func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            \/\/ Notification for JivoSDK, just pass this result into completionHandler","            completionHandler(result)","        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    override func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","}"],"syntax":"swift","fileName":"AppDelegate.swift"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics#How-to-handle-user-session":{"identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics#How-to-handle-user-session","title":"How to handle user session","kind":"section","type":"section","role":"pseudoSymbol","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"url":"\/tutorials\/jivosdk\/native_usage_basics#How-to-handle-user-session"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/react_usage_basics":{"abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"title":"Basic usage","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/react_usage_basics","kind":"project","role":"project","type":"topic","url":"\/tutorials\/jivosdk\/react_usage_basics"},"flutter.user_manager_02.dart.sample":{"type":"file","highlights":[{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29}],"fileType":"dart","identifier":"flutter.user_manager_02.dart.sample","syntax":"dart","content":["import 'package:jivosdk_plugin\/bridge.dart';","","class User {","    String email;","    String jwt;","    String? fullName;","    String? phoneNumber;","    User(this.email, this.jwt, this.fullName, this.phoneNumber);","}","","class UserManager {","    User? user;","    ","    void updateUser(User? user) {","        this.user = user;","        _configureJivoSession(