JivoSDK/native_usage_basics#How-to-handle-user-session":{"role":"pseudoSymbol","url":"\/tutorials\/jivosdk\/native_usage_basics#How-to-handle-user-session","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"type":"section","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics#How-to-handle-user-session","kind":"section","title":"How to handle user session"},"react.app_delegate_02.swift.sample":{"fileType":"swift","type":"file","identifier":"react.app_delegate_02.swift.sample","highlights":[{"line":3},{"line":14},{"line":15},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":34},{"line":39},{"line":43},{"line":47},{"line":48},{"line":49},{"line":50},{"line":51},{"line":52},{"line":53},{"line":54},{"line":55},{"line":56},{"line":57},{"line":58},{"line":59},{"line":60},{"line":61},{"line":62},{"line":63},{"line":64},{"line":65},{"line":66}],"content":["import Foundation","import UIKit","import UserNotifications","import JivoSDK","","#if DEBUG","#if FB_SONARKIT_ENABLED","import FlipperKit","#endif","#endif","","@UIApplicationMain","final class AppDelegate: UIResponder",", UIApplicationDelegate",", UNUserNotificationCenterDelegate {","    func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil) -> Bool {","        initializeFlipper(with: application)","        ","        \/\/ ...","        ","        if Jivo.notifications.handleLaunch(options: launchOptions) {","            \/\/ Launched by JivoSDK notification","        }","        ","        UNUserNotificationCenter.current().delegate = self","        return true","    }","    ","    func application(_ application: UIApplication, didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Jivo.notifications.setPushToken(data: deviceToken)","    }","    ","    func application(_ application: UIApplication, didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(data: nil)","    }","    ","    func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            \/\/ Notification for JivoSDK, just pass this result into completionHandler","            completionHandler(result)","        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","    ","    private func initializeFlipper(with application: UIApplication) {","        \/\/ ...","    }","}"],"syntax":"swift","fileName":"AppDelegate.swift"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics#How-to-handle-Push-Notifications":{"role":"pseudoSymbol","url":"\/tutorials\/jivosdk\/native_usage_basics#How-to-handle-Push-Notifications","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"type":"section","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics#How-to-handle-Push-Notifications","kind":"section","title":"How to handle Push Notifications"},"doc://org.cocoapods.JivoSDK/tutorials/table/Native-app":{"type":"topic","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table\/Native-app","title":"Native app","kind":"article","abstract":[],"url":"\/tutorials\/table\/native-app","role":"article"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/flutter_usage_basics#How-to-handle-user-session":{"role":"pseudoSymbol","abstract":[{"text":"Some scenarios of how to use SDK","type":"text"}],"url":"\/tutorials\/jivosdk\/flutter_usage_basics#How-to-handle-user-session","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/flutter_usage_basics#How-to-handle-user-session","kind":"section","title":"How to handle user session","type":"section"},"react.user_manager_01.js.sample":{"type":"file","content":["import Jivo from 'rtn-jivo-sdk';","","class UserManager {","    constructor() {","        this.user = null","    }","","    updateUser(user) {","        this.user = user","        this._configureJivoSession()","    }","","    _configureJivoSession() {","        if (this.user) {","            Jivo.session.setup(\"YOUR_CHANNEL_ID\", this.user.jwt)","        }","        else {","            Jivo.session.shutDown()","        }","    }","}"],"highlights":[{"line":1},{"line":2},{"line":10},{"line":12},{"line":13},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20}],"identifier":"react.user_manager_01.js.sample","syntax":"js","fileName":"UserManager.js","fileType":"js"},"react.app_delegate_00.swift.sample":{"highlights":[],"fileType":"swift","identifier":"react.app_delegate_00.swift.sample","syntax":"swift","content":["import Found