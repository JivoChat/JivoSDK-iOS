emoteNotification(userInfo: userInfo) {","        completionHandler(result)","        return","    }","","    \/\/ ... your existing logic ...","}","","func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","    if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","        completionHandler(options)","        return","    }","","    \/\/ ... your existing logic ...","}","","func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","    if Jivo.notifications.didReceive(response: response) {","        Jivo.display.present(over: rootViewController)","        completionHandler()","    }","","    \/\/ ... your existing logic ...","}"],"type":"codeListing","syntax":"swift"}]},{"content":[{"text":"May be interesting for you:","type":"heading","level":2,"anchor":"May-be-interesting-for-you"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"isActive":true,"type":"reference","identifier":"https:\/\/jivochat.github.io\/JivoSDK-iOS\/tutorials\/jivosdk\/native_usage_basics"}]}]},{"content":[{"inlineContent":[{"overridingTitleInlineContent":[{"text":"User token and history","type":"text"}],"identifier":"https:\/\/jivochat.github.io\/JivoSDK-iOS\/documentation\/jivosdk\/common_user_token","type":"reference","overridingTitle":"User token and history","isActive":true}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"isActive":true,"identifier":"https:\/\/jivochat.github.io\/JivoSDK-iOS\/documentation\/jivosdk\/jvdisplaycontroller","type":"reference"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"identifier":"https:\/\/jivochat.github.io\/JivoSDK-iOS\/documentation\/jivosdk\/common_project_config\/#Lo