tionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        Jivo.notifications.didReceive(response: response)","        completionHandler()","    }","}"],"identifier":"native.app_delegate_02.swift.sample","syntax":"swift","highlights":[{"line":4},{"line":11},{"line":12},{"line":13},{"line":14},{"line":15},{"line":20},{"line":24},{"line":28},{"line":29},{"line":30},{"line":31},{"line":32},{"line":33},{"line":34},{"line":35},{"line":39},{"line":40},{"line":41},{"line":42},{"line":43},{"line":44},{"line":45},{"line":46},{"line":50}],"type":"file","fileName":"AppDelegate.swift","fileType":"swift"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics":{"identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"type":"topic","url":"\/tutorials\/jivosdk\/native_usage_basics","role":"project","kind":"project","title":"Basic usage"},"native.profile_view_controller_00.swift.sample":{"fileName":"ProfileViewController.swift","highlights":[],"fileType":"swift","type":"file","syntax":"swift","content":["import Foundation","import UIKit","","fileprivate enum ProfileMenuItem: Int {","    case faq","    case website","}","","final class ProfileViewController: UITableViewController, UITableViewDelegate {","    \/\/ ...","    ","    func tableView(