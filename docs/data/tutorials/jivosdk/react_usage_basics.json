ication userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            \/\/ Notification for JivoSDK, just pass this result into completionHandler","            completionHandler(result)","        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","    ","    private func initializeFlipper(with application: UIApplication) {","        \/\/ ...","    }","}"]},"doc://org.cocoapods.JivoSDK/tutorials/table/React-Native-app":{"abstract":[],"url":"\/tutorials\/table\/react-native-app","type":"topic","role":"article","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table\/React-Native-app","title":"React Native app","kind":"article"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/flutter_usage_basics#How-to-handle-Push-Notifications":{"title":"How to handle Push Notifications","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/flutter_usage_basics#How-to-handle-Push-Notifications","url":"\/tutorials\/jivosdk\/flutter_usage_basics#How-to-handle-Push-Notifications","type":"section","role":"pseudoSymbol","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"kind":"section"},"doc://org.cocoapods.JivoSDK/documentation/JivoSDK/common_user_token":{"type":"topic","url":"\/documentation\/jivosdk\/common_user_token","role":"article","abstract":[{"type":"text","text":"Required for keeping chat history alive"}],"kind":"article","identifier":"doc:\/\/org.cocoapods.JivoSDK\/documentation\/JivoSDK\/common_user_token","title":"User Token and JWT"},"doc://org.cocoapods.JivoSDK/tutorials/table":{"abstract":[],"url":"\/tutorials\/table","type":"topic","role":"overview","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table","title":"JivoSDK Samples","kind":"overview"},"react.user_manager_02.js.sample":{"syntax":"js","content":["import Jivo from 'rtn-jivo-sdk';","","class UserManager {","    constructor() {","        this.user = null","    }","","    updateUser(user) {","        this.user = user","        this._configureJivoSession()","    }","","    _configureJivoSession() {","        if (this.user) {","            const info = {","                'name': this.user.full_name,","                'email': this.user.email,","                'phone': this.user.phone_number","            }","            ","            Jivo.session.setup(\"YOUR_CHANNEL_ID\", this.user.jwt)","            Jivo.session.setContactInfo(info)","        }","        else {","            Jivo.session.shutDown()","        }","    }","}"],"identifier":"react.user_manager_02.js.sample","fileType":"js","type":"file","fileName":"UserManager.js","highlights":[{"line":15},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":22}]},"react.user_